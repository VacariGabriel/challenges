{
  "env": {
    "commonjs": true,
    "es2020": true,
    "node": true
  },
  "globals": {
    "expect": "readonly",
    "describe": "readonly",
    "it": "readonly"
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "error",
    "no-param-reassign": 0,
    "consistent-return": 0,
    "array-callback-return": 0
  }
}
